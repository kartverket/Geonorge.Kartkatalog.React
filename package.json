  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired --max_old_space_size=8192 build",
    "postbuild": "react-snap --disable-dev-shm-usage",
    "generate-sw": "workbox generateSW --root=build workbox-config.js",
    "test": "jest --config jest.config.js --verbose",
    "test:watch": "jest --config jest.config.js --verbose --watchAll",
    "test:status": "jest --config jest.config.js --verbose --coverage",
    "test:update": "jest --config jest.config.js -u"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-extensions"
    ],
    "cacheAjaxRequests": true,
    "http2PushManifest": true,
    "include": [
      "/shell.html"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ]
}
